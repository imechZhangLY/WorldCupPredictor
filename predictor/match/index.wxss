html{
  padding:0;
  margin:0;

}
.container {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url("https://raw.githubusercontent.com/imechZhangLY/WorldCup/master/images/Russian.png");
}
.match{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.head {
	width: 600rpx;
	height: 100rpx;
	background-color: #bcb188;
	text-align: center;
	line-height: 100rpx;
	color: white;
	font-size: 1.5em;
}
.body {
	width: 600rpx;
	height: 350rpx;
	background: linear-gradient(90deg,#f1ece3,#f1ece3 0,#fff 53.08%,#f1ece3);
}
.group {
	margin-top: 50rpx;
	text-align: center;
}
.content {
	display: flex;
	justify-content: space-between;
	margin-top: 30rpx;
	padding: 20rpx 30rpx;
}
.score {
	display: flex;
	height: 90rpx;
	align-items: center;
}
input {
 border: 1px solid rgb(200, 200, 200);
 background-color: #fff;
 width: 50rpx;
 margin-left: 20rpx;
 margin-right: 20rpx;
}
.flag {
	width: 180rpx;
}
.tips {
	margin-top: 30rpx;
	text-align: center;
	color: rgb(180, 180, 180);
}
.foot {
	width: 600rpx;
	height: 300rpx;
	background-size: contain;
	background-repeat: no-repeat;
	background-image: url(data:image/png;base64,);
}
button {
	margin-top: 80rpx;
}